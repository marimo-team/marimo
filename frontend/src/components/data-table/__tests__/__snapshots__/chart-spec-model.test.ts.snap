        "value": "A",
        "xEnd": 10,
        "xMid": 5,
        "xStart": 0,
      },
      {
        "count": 30,
        "proportion": 0.75,
        "value": "B",
        "xEnd": 40,
        "xMid": 25,
        "xStart": 10,
      },
    ],
  },
  "height": 100,
  "layer": [
    {
      "encoding": {
        "color": {
          "condition": {
            "test": "datum.value == "None" || datum.value == "null"",
            "value": "#cc4e00",
          },
          "value": "#027864",
        },
        "opacity": {
          "condition": [
            {
              "param": "hover_bar",
              "value": 1,
            },
          ],
          "value": 0.6,
        },
        "tooltip": [
          {
            "field": "value",
            "title": "string",
            "type": "nominal",
          },
          {
            "field": "count_with_percent",
            "title": "Count",
            "type": "nominal",
          },
        ],
        "x": {
          "axis": null,
          "field": "xStart",
          "type": "quantitative",
        },
        "x2": {
          "field": "xEnd",
          "type": "quantitative",
        },
      },
      "mark": {
        "type": "bar",
      },
      "params": [
        {
          "name": "hover_bar",
          "select": {
            "clear": "mouseout",
            "on": "mouseover",
            "type": "point",
          },
        },
      ],
      "transform": [
        {
          "as": "count_with_percent",
          "calculate": "format(datum.count, ',d') + ' (' + format(datum.count / 40 * 100, '.1f') + '%)'",
        },
      ],
    },
    {
      "encoding": {
        "text": {
          "field": "clipped_text",
        },
        "x": {
          "axis": null,
          "field": "xMid",
          "type": "quantitative",
        },
      },
      "mark": {
        "clip": true,
        "color": "white",
        "ellipsis": " ",
        "fontSize": 8.5,
        "type": "text",
      },
      "transform": [
        {
          "as": "clipped_text",
          "calculate": "datum.proportion > 0.5 ? slice(datum.value, 0, 8) : datum.proportion > 0.2 ? slice(datum.value, 0, 3) : datum.proportion > 0.1 ? slice(datum.value, 0, 1) : ''",
        },
      ],
    },
  ],
  "transform": [
    {
      "as": "percent",
      "calculate": "datum.count / 40",
    },
  ],
}
`;

